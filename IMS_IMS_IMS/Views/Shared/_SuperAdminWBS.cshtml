<script>


     $(document).on("click", "a[name='ManageUser_name']", function (event) {
        event.preventDefault();
         $('#renderbody').load('@Url.Action("ManageUserWBS","WBS")');

     });

     $(document).on("click", "a[name='AddDetails_Admin']", function (event) {
        event.preventDefault();
         $('#renderbody').load('@Url.Action("AddDetailsAdmin","WBSAdmin")');

    });


     $(document).on("click", "a[name='SuperAdminWBSHomePage']", function (event) {
         event.preventDefault();
        
         '@Url.Action("AddDetailsAdmin","WBSAdmin")';
         

     });

    

</script>
