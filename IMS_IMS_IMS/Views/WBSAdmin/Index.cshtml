<script>

        var responseData = '';
        var ShowPumpSetDetails = function () {
            var location = "";
            location=window.localStorage.getItem('LocationStorage');
            var Loc = {location: location }
            var data = JSON.stringify(Loc);

            $.ajax({

            type: "POST",
            url: "/WBSAdmin/ShowPumpSetDetail",
            contentType: 'application/json; charset=utf-8',
            dataType: "json",
            data: data,
            success: function (response) {

                response = JSON.parse(BindDataTablePumpSet(response));


            $("#MyDataTablePumpSet").append(response)


        }
});
}

        var BindDataTablePumpSet = function (response) {



                $("#MyDataTablePumpSet").DataTable({

                    responsive: true,
                    "aaData": response,
                    "aoColumns": [

                        { "mData": "ItemCode" },
                        //{ "mData": "ItemName" },
                        { "mData": "Location" },
